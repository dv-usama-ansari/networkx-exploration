{
    "databases": [
      {
        "id": "aevidencedb",
        "dbEngine": "postgresql",
        "schemas": [
          {
            "name": "ordino",
            "entities": [
              {
                "name": "Transcript",
                "description": "The Transcript entity represents individual RNA transcripts associated with genes, capturing their expression levels (TPM) across GTEx samples. It links to the Genes entity and provides detailed transcript-level data and sample-specific expression. This entity enables analysis of transcript diversity and tissue-specific expression patterns.",
                "color": "#008080",
                "tableName": "transcript",
                "enableQueryAll": false,
                "columns": [
                  {
                    "columnName": "ensemblid",
                    "label": "Ensembl ID",
                    "width": 200,
                    "initialRanking": true,
                    "type": "string",
                    "primary": true
                  },
                  {
                    "columnName": "transcript_id",
                    "label": "Transcript ID",
                    "width": 200,
                    "initialRanking": true,
                    "type": "string",
                    "searchable": true
                  },
                  {
                    "columnName": "gtex_sample_id",
                    "label": "GTEx sample ID",
                    "width": 200,
                    "initialRanking": true,
                    "type": "string",
                    "searchable": true
                  },
                  {
                    "columnName": "tpm",
                    "label": "mRNA expression log2(TPM + 1)",
                    "initialRanking": true,
                    "type": "number",
                    "searchable": false
                  }
                ]
              }
            ]
          }
        ]
      }
    ],
    "relations": [
      {
        "type": "1-n",
        "source": {
          "id": "aevidencedb.ordino.genes",
          "key": "ensemblid"
        },
        "target": {
          "id": "aevidencedb.ordino.transcript",
          "key": "ensemblid"
        },
        "workbench": {
          "minSelection": 0,
          "maxSelection": 2000
        },
        "bidirectional": false
      }
    ],
