          "filter": {
            "col": "species",
            "op": "IN",
            "val": "human"
          },
          "views": [
            {
              "name": "COSMIC gene",
              "site": "https://cancer.sanger.ac.uk/cosmic/gene/analysis?ln=${genesymbol}&genome=38",
