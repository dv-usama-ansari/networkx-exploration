                    },
                    {
                      "name": 4,
                      "label": "(UniProt loc) Targets with UniProt 'Subcellular location' terms indicative for plasma membrane, extracellular region/matrix, or secretion - high confidence"
                    },
                    {
                      "name": 5,
                      "label": "(GO CC) Targets with GO 'Cellular component' terms indicative for plasma membrane, extracellular region/matrix, or secretion - high confidence [‘ECO:0000269’ or ‘ECO:0000305’] Bucket 6 (UniProt loc) Targets with UniProt 'Subcellular location' terms indicative for plasma membrane, extracellular region/matrix, or secretion - medium confidence"
                    },
                    {
                      "name": 7,
                      "label": "(UniProt SigP or TMHMM): Targets with predicted Signal Peptide or Trans-membrane regions, and not destined to organelles"
                    },
                    {
                      "name": 8,
                      "label": "(GO CC) Targets with GO 'Cellular component' terms indicative for plasma membrane, extracellular region/matrix, or secretion - medium confidence"
                    },
                    {
                      "name": 9,
                      "label": "(Human Protein Atlas) Targets with main location “Plasma membrane” - high confidence"
